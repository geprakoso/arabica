{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "afatmustafa/blade-hugeicons": "^1.0",
        "akaunting/money": "^6.0",
        "alperenersoy/filament-export": "^3.0",
        "ariaieboy/filament-currency": "^1.13",
        "awcodes/filament-badgeable-column": "^2.3",
        "barryvdh/laravel-dompdf": "^3.1",
        "bezhansalleh/filament-panel-switch": "^1.1",
        "bezhansalleh/filament-shield": "^3.9",
        "codeat3/blade-carbon-icons": "^2.37",
        "codeat3/blade-eos-icons": "^1.15",
        "dotswan/filament-map-picker": "^1.8",
        "eightynine/filament-advanced-widgets": "^3.0",
        "eightynine/filament-excel-import": "^3.1",
        "eightynine/filament-reports": "^3.1",
        "emmanpbarrameda/filament-take-picture-field": "^1.1",
        "filament/filament": "^3.3",
        "guava/calendar": "*",
        "icetalker/filament-table-repeater": "^1.4",
        "joaopaulolndev/filament-edit-profile": "^1.0",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "laravolt/indonesia": "^0.38.0",
        "mokhosh/filament-kanban": "^2.11",
        "nuxtifyts/dash-stack-theme": "^1.3",
        "oriondevelops/filament-greeter": "^0.2.2",
        "shanerbaner82/panel-roles": "^0.0.3",
        "silviolleite/laravelpwa": "^2.0",
        "solution-forest/tab-layout-plugin": "^2.2",
        "spatie/eloquent-sortable": "^4.5",
        "tomatophp/filament-media-manager": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.8",
        "pestphp/pest-plugin-laravel": "^3.2",
        "pestphp/pest-plugin-livewire": "^3.0",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "AlperenErsoy\\FilamentExport\\": "app/Overrides/FilamentExport/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run build"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "./vendor/bin/pest"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "github-oauth": {},
        "http-timeout": 600,
        "lock": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
